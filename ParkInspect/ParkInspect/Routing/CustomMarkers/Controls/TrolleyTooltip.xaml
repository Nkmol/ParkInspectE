<UserControl x:Class="ParkInspect.View.Controls.TrolleyTooltip"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Background="Transparent" Height="186" Width="226" IsHitTestVisible="False">
    <Grid>
        <Rectangle Stroke="Black" RadiusX="22" RadiusY="22" Fill="AliceBlue" Margin="0,0,10,69"></Rectangle>
        <StackPanel>
            <Label x:Name="Place" Content="Label" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
            <Label x:Name="Name" Content="Label" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
            <Label x:Name="Status" Content="Label" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        </StackPanel>
    </Grid>
</UserControl>
