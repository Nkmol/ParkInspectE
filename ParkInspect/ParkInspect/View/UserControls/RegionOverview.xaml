<UserControl x:Class="ParkInspect.View.UserControls.RegionOverview"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ParkInspect.View.UserControls"
             xmlns:services="clr-namespace:ParkInspect.Services"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             DataContext="{Binding Regions, Source={StaticResource Locator}}">
    <ListView ItemsSource="{Binding Regions}" DisplayMemberPath="Name" SelectedItem="{Binding SelectedRegion}">
        
    </ListView>
</UserControl>
