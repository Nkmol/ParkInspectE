@using ParkInspect.WEB.Controllers
@{
    ViewBag.Title = "Home Page";
}

<div><table style="width:100%">
  @{
      foreach (var pdf in ViewBag.pdf)
      {
          <tr>
              <td>
                  
                  <iframe id="fred" style="border:1px solid #666CCC" title="PDF in an i-Frame" src="@Url.Action("GetPdf", "Home" , new {path = pdf})" frameborder="1" scrolling="auto" height="1100" width="850"></iframe>
                  


              </td> 

          </tr>
      }
  }
</table></div>



